  "description": "## my-favorite-podcasts-api",
  "main": ".index.js",
  "scripts": {
    "dev": "./node_modules/.bin/concurrently \"npm run dev:client\" \"npm run dev:server\"",
    "dev:client": "./node_modules/.bin/webpack --progress --inline --config webpack.dev.js",
    "dev:server": "./node_modules/.bin/env-cmd ./node_modules/.bin/babel-watch server.js",
    "dev:migrate": "./node_modules/.bin/env-cmd ./node_modules/.bin/sequelize db:migrate --env development --config ./configs/sequelize.js",
    "eslint": "./node_modules/.bin/eslint --format codeframe .",
    "lint": "npm run eslint && npm run puglint",
    "lint:fix": "./node_modules/.bin/eslint --fix --format codeframe --fix .",
    "puglint": "./node_modules/.bin/pug-lint .",
    "test": "./node_modules/.bin/env-cmd ./node_modules/.bin/mocha tests/**/*.tests.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BMAnderson1913/my-favorite-podcasts-api.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/BMAnderson1913/my-favorite-podcasts-api/issues"
  },
  "homepage": "https://github.com/BMAnderson1913/my-favorite-podcasts-api#readme",
  "devDependencies": {
    "@babel/core": "^7.10.2",
    "@babel/preset-env": "^7.10.2",
    "@babel/preset-react": "^7.10.1",
    "babel-loader": "^8.1.0",
    "babel-watch": "^7.0.0",
    "chai": "^4.2.0",
    "concurrently": "^5.2.0",
    "env-cmd": "^10.1.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.20.0",
    "mocha": "^7.2.0",
    "pug-lint": "^2.6.0",
    "sinon": "^9.0.2",
    "sinon-chai": "^3.5.0",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "express": "^4.17.1",
    "mysql2": "^2.1.0",
    "pug": "^2.0.4",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "sequelize": "^5.21.11",
    "sequelize-cli": "^5.5.1"
  }
